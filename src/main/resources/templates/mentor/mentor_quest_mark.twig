<section class="users-table">
    <div>
    {% if (message.length > 0) %}
       <img src="/static/assets/pics/web/OK.png" alt="OK" style="height:20px;width:20px;display:inline-block" ><h4>{{ message }}</h4>
    {% endif %}
   </div>
    <h3>Quest</h3>
    <table style="width:100%">
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Description</th>
        <th>Reward</th>
      </tr>
        {% set i = 0 %}
        {% for quest in questsList %}
            {% set i = i + 1 %}
            <tr>
                <td>{{ i }}</td>
                <td>{{ quest.getName() }}</td>
                <td>{{ quest.getDescription() }}</td>
                <td>{{ quest.getReward() }}</td>
                <td><a class="btn" id="edit-btn" href="/mentor/groups/quest/{{ studentLogin }}/mark/{{ quest.getName() }}">Mark</td>
            </tr>
        {% endfor %}
    </table>
</section>
